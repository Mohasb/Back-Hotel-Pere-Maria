{
  "$jsonSchema": {
    "bsonType": "object",
    "required": ["room", "user", "rating", "comment"],
    "properties": {
      "room": {
        "bsonType": "int"
      },
      "user": {
        "bsonType": "object",
        "required": ["email", "name"],
        "properties": {
          "email": {
            "bsonType": "string"
          },
          "name": {
            "bsonType": "string"
          }
        }
      },
      "rating": {
        "bsonType": "int",
        "minimum": 1,
        "maximum": 5
      },
      "comment": {
        "bsonType": "string"
      }
    }
  }
}
